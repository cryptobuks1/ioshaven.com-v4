{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/v4"
        }
    ],
    "require": {
        "php": "^7.3.13",
        "anhskohbo/no-captcha": "^3.0",
        "david-griffiths/nova-dark-theme": "^1.0",
        "doctrine/dbal": "^2.9",
        "fedeisas/laravel-mail-css-inliner": "^2.2",
        "fideloper/proxy": "^4.0",
        "gregoriohc/laravel-nova-theme-responsive": "^0.8.0",
        "guzzlehttp/guzzle": "^6.3",
        "inspheric/nova-url-field": "^1.4",
        "ioshaven/v4": "*",
        "jdenticon/jdenticon": "^0.10.0",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^5.8.0",
        "laravel/nova": "~2.0",
        "laravel/tinker": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "predis/predis": "^1.1",
        "rap2hpoutre/laravel-log-viewer": "^1.2",
        "saumini/count": "^1.0",
        "spatie/laravel-sitemap": "^5.2",
        "symfony/translation-contracts": "^1.1.6"    
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
